{
  "name": "multicolour",
  "version": "0.6.10",
  "description": "Generate your REST API with JSON and use almost any DB tech.",
  "keywords": [
    "Multicolour",
    "REST API",
    "generator",
    "ORM"
  ],
  "main": "index.js",
  "preferGlobal": true,
  "bin": {
    "multicolour": "cli.js",
    "multicolor": "cli.js",
    "color": "cli.js"
  },
  "scripts": {
    "linter": "eslint ./**/*.js",
    "unit-tests": "tape tests/all.js | tap-spec",
    "integration-tests": "newman run tests/integration/postman_collection.json",
    "test": "npm run linter && npm run unit-tests",
    "test-coverage": "istanbul cover tape tests/*.js --recursive --report lcov -x '**/cli/**' -x '**/waterline-fixes/**' | tap-spec",
    "changelog": "github_changelog_generator -u Multicolour -p multicolour"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Multicolour/multicolour.git"
  },
  "author": "newworldcode <hello@newworld.codes> (https://newworld.codes)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Multicolour/multicolour/issues"
  },
  "homepage": "https://github.com/Multicolour/multicolour#readme",
  "dependencies": {
    "@newworldcode/talkie": "^1.0.9",
    "chalk": "^1.1.3",
    "commander": "^2.10.0",
    "cross-spawn": "^5.1.0",
    "debug": "^2.6.8",
    "fs-extra": "^3.0.1",
    "inquirer": "^3.1.1",
    "joi": "^10.6.0",
    "jsocrud": "^2.0.3",
    "mkdirp": "^0.5.1",
    "npm-registry-client": "^8.4.0",
    "pluralize": "^5.0.0",
    "replacestream": "^4.0.2",
    "uuid": "^3.1.0",
    "validate-npm-package-name": "^3.0.0",
    "waterline": "^0.11.11",
    "waterline-joi": "^1.1.1"
  },
  "engine-strict": true,
  "engineStrict": true,
  "engines": {
    "node": ">= 4.5.0"
  },
  "devDependencies": {
    "async": "^1.5.2",
    "eslint": "^4.1.1",
    "istanbul": "^0.4.5",
    "newman": "^3.7.3",
    "sails-memory": "^0.10.7",
    "snyk": "^1.36.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.7.0"
  }
}
